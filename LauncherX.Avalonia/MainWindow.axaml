<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sty="using:FluentAvalonia.Styling"
        xmlns:ui="using:FluentAvalonia.UI.Controls"
        xmlns:uip="using:FluentAvalonia.UI.Controls.Primitives"
        mc:Ignorable="d"
        x:Class="LauncherX.Avalonia.MainWindow"
         Title="LauncherX - Organize all your stuff!" Height="533" Width="800" MinHeight="400" MinWidth="650">
  <Panel>
    <AutoCompleteBox Margin="20,20,20,0" VerticalAlignment="Top" Watermark="Search through everything in LauncherX" />

    <ui:NavigationView HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="20,62,20,20" PaneDisplayMode="Top" Header="My files, folders, and websites" IsBackButtonVisible="True" IsBackEnabled="True" FontSize="24" >
      <ui:NavigationView.MenuItems>
        <ui:NavigationViewItem Content="All Items"/>
        <ui:NavigationViewItem Content="Files"/>
        <ui:NavigationViewItem Content="Folders"/>
        <ui:NavigationViewItem Content="Websites"/>
      </ui:NavigationView.MenuItems>
    </ui:NavigationView>
  </Panel>
</Window>
